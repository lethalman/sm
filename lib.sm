map = f s:
	x, ns = s;
	if eos? x
		then eos
		else [f x, map f ns];

filter = f s:
	x, ns = s;
	if eos? x then eos
	else if f x then [x, filter f ns]
	else filter f ns;

heads = n s:
	x, ns = s;
	if eos? x then eos
	else if n < 1 then eos
	else [x, heads (n-1) ns];
head = heads 1;

flip = f x y: f y x;
	
test = stream [1,2,3];

print 10
	(map (x:x+1) test)
	(filter (x:x < 3) test)
	(filter (x:x > 1) test)
	(head test)